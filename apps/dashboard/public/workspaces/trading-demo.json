{
  "id": "demo-trading-001",
  "name": "Trading Analytics",
  "serverUrl": "ws://localhost:8765",
  "activeDashboardId": "dash-trading-main",
  "queries": [
    {
      "id": "q-trades-recent",
      "name": "Recent Trades",
      "code": "(take trades -20)",
      "isRunning": false
    },
    {
      "id": "q-trade-count",
      "name": "Trade Count",
      "code": "(count trades)",
      "isRunning": false
    },
    {
      "id": "q-ohlc-candles",
      "name": "Price Candles",
      "code": "(select {t: (first (eval (as 'symbol \"timestamp\"))) open: (first price) high: (max price) low: (min price) close: (last price) from: trades by: (xbar (eval (as 'symbol \"timestamp\")) 5000000000)})",
      "isRunning": false
    }
  ],
  "dashboards": [
    {
      "id": "dash-trading-main",
      "name": "Trading Overview",
      "createdAt": 1704067200000,
      "updatedAt": 1704067200000,
      "widgets": [
        {
          "id": "w-trades-grid",
          "type": "grid",
          "title": "Recent Trades",
          "binding": {
            "queryId": "q-trades-recent",
            "refreshInterval": 5000,
            "autoRun": true
          },
          "config": {},
          "position": { "x": 0, "y": 0, "w": 7, "h": 6 }
        },
        {
          "id": "w-candlestick",
          "type": "chart",
          "title": "Price Candles",
          "binding": {
            "queryId": "q-ohlc-candles",
            "refreshInterval": 5000,
            "autoRun": true
          },
          "config": { "chartType": "candlestick" },
          "position": { "x": 7, "y": 0, "w": 5, "h": 4 }
        },
        {
          "id": "w-trade-count",
          "type": "text",
          "title": "Total Trades",
          "binding": {
            "queryId": "q-trade-count",
            "refreshInterval": 5000,
            "autoRun": true
          },
          "config": { "fontSize": 64, "suffix": " trades" },
          "position": { "x": 7, "y": 4, "w": 5, "h": 2 }
        }
      ]
    }
  ]
}
